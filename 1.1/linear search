#include <stdio.h>

int main() {
    int n, key, found = 0;

    // Read size of array
    scanf("%d", &n);

    int arr[n];

    // Read array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read element to search
    scanf("%d", &key);

    // Linear search
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            printf("found at position %d\n", i);
            found = 1;
            break;
        }
    }

    // If not found
    if (!found) {
        printf("%d not found\n", key);
    }

    return 0;
}

